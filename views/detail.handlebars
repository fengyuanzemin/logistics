<div class="container">
    {{>header}}
    {{>alert}}
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <table class="table table-striped table-hover">
            <thead class="thead-inverse ">
            <tr>
                <th>时间</th>
                <th>状态</th>
            </tr>
            </thead>
            <tbody>
            {{# each detail}}
                <tr>
                    <td>{{timeFormat createdAt}}</td>
                    <td>{{action}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
</div>